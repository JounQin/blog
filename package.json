    "glob": "^7.1.6",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.2.4",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
