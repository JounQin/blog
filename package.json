    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "style-resources-loader": "^1.3.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "typeface-lato": "^0.0.75",
    "typescript": "^3.7.2",
    "url-loader": "^2.3.0",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "extends @1stg/browserslist-config/modern"
  ],
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [
      ".dev.env",
      "server/router"
    ]
  },
  "prettier": "@1stg/prettier-config/vue",
  "remarkConfig": {
    "plugins": [
      "@1stg/remark-config"
    ]
  },
  "renovate": {
    "extends": [
      "@1stg"
    ]
  },
  "stylelint": {
    "extends": [
      "@1stg/stylelint-config/scss",
      "@1stg/stylelint-config/modules"
    ]
  }
}
