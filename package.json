{
  "name": "@1stg/blog",
  "version": "0.0.0",
  "description": "a Blog system built on GitHub API with Vue SSR",
  "repository": "git@github.com:JounQin/blog.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "rimraf dist && run-p build:vue:client build:vue:server build:server build:template",
    "build:server": "env-cmd -f .dev.env env-cmd -f .prod.env webpack --config build/server.ts --colors --progress -p",
    "build:template": "mkdir -p dist/static && ts-node build/template",
    "build:vue:client": "env-cmd -f .dev.env env-cmd -f .prod.env webpack --config build/vue-client.ts --colors --progress -p",
    "build:vue:server": "env-cmd -f .dev.env env-cmd -f .prod.env webpack --config build/vue-server.ts --colors --progress -p",
    "dev": "run-p server:core watch",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts,vue -f friendly",
    "lint:style": "stylelint --cache 'src/**/*.{scss,vue}'",
    "pm2": "yarn sync:pull && pm2 start --name=blog npm -- start",
    "prune": "yarn --production --ignore-optional --ignore-scripts --prefer-offline --force",
    "server": "ts-node -T -r tsconfig-paths/register server",
    "server:core": "env-cmd -f .prod.env env-cmd -f .dev.env yarn server",
    "server:router": "ts-node -T -r tsconfig-paths/register server/router/dev",
    "start": "env-cmd -f .dev.env env-cmd -f .prod.env node dist/server",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh",
    "watch": "nodemon --exec \"env-cmd -f .prod.env env-cmd -f .dev.env yarn server:router\""
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "date-fns": "^2.8.0",
    "debug": "^4.1.1",
    "env-cmd": "^10.0.1",
    "github-markdown-css": "^3.0.1",
    "google-translate-api": "^2.3.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "invert-color": "^2.0.0",
    "koa": "^2.11.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static-cache": "^5.1.2",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "memory-fs": "^0.5.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.1",
    "serialize-javascript": "^2.1.0",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vue-translator": "^0.8.0",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.1.16",
    "@packtracker/webpack-plugin": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static-cache": "^5.1.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/memory-fs": "^0.3.2",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.12",
    "@types/node-fetch": "^2.5.4",
    "@types/pug": "^2.0.4",
    "@types/qs": "^6.9.0",
    "@types/serialize-javascript": "^1.5.0",
    "@types/shelljs": "^0.8.6",
    "@types/sw-precache-webpack-plugin": "^0.11.0",
