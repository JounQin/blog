{
  "name": "@1stg/blog",
  "version": "0.0.0",
  "description": "a Blog system built on GitHub GraphQL API with Vue SSR",
  "repository": "git@github.com:JounQin/blog.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && env-cmd run-p build:**",
    "build:server": "webpack --config build/server.ts --colors --progress -p",
    "build:template": "ts-node build/template",
    "build:vue:client": "env-cmd webpack --config build/vue-client.ts --colors --progress -p",
    "build:vue:server": "env-cmd webpack --config build/vue-server.ts --colors --progress -p",
    "dev": "env-cmd run-p server watch",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts,vue -f friendly",
    "lint:style": "stylelint --cache 'src/**/*.{scss,vue}'",
    "lint:tsc": "tsc --noEmit",
    "pm2": "yarn sync:pull && pm2 start --name=blog npm -- start",
    "postinstall": "cpy env.js . --rename=.env.js && yarn-deduplicate || exit 0",
    "prune": "yarn --production --ignore-optional --ignore-scripts --prefer-offline --force",
    "server": "ts-node -T -r tsconfig-paths/register server",
    "server:router": "ts-node -T -r tsconfig-paths/register server/router/dev",
    "start": "env-cmd node dist/server",
    "type-coverage": "type-coverage --detail --ignore-files 'src/types/schema.ts' --ignore-files '*.d.ts' --skip-catch --strict",
    "watch": "nodemon"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.2",
    "date-fns": "^2.9.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "github-markdown-css": "^3.0.1",
    "google-translate-api": "^2.3.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "invert-color": "^2.0.0",
    "koa": "^2.11.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.6",
    "koa-session": "^5.12.3",
    "koa-static-cache": "^5.1.2",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "memory-fs": "^0.5.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.1",
    "serialize-javascript": "^2.1.2",
    "uuid": "^3.4.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vue-translator": "^0.8.0",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.1.21",
    "@packtracker/webpack-plugin": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static-cache": "^5.1.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/memory-fs": "^0.3.2",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.5.0",
    "@types/node-fetch": "^2.5.4",
    "@types/pug": "^2.0.4",
    "@types/qs": "^6.9.0",
    "@types/serialize-javascript": "^1.5.0",
    "@types/shelljs": "^0.8.6",
    "@types/sw-precache-webpack-plugin": "^0.11.0",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.41.3",
    "@types/webpack-env": "^1.15.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.0",
    "bootstrap": "^4.4.1",
    "cpy-cli": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^4.0.2",
    "glob": "^7.1.6",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.2.5",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-url": "^8.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "purgecss-webpack-plugin": "^2.0.5",
    "purgecss-whitelister": "^2.4.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "style-resources-loader": "^1.3.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "type-coverage": "^2.4.0",
    "typeface-lato": "^0.0.75",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "yarn-deduplicate": "^1.1.1"
  },
  "browserslist": [
    "extends @1stg/browserslist-config/modern"
  ],
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn server:router",
    "ext": "ts",
    "watch": [
      ".env.local",
      "server/router"
    ]
  },
  "prettier": "@1stg/prettier-config/vue",
  "remarkConfig": {
    "plugins": [
      "@1stg/remark-config"
    ]
  },
  "renovate": {
    "extends": [
      "@1stg"
    ]
  },
  "stylelint": {
    "extends": [
      "@1stg/stylelint-config/scss",
      "@1stg/stylelint-config/modules"
    ]
  },
  "type-coverage": {
    "atLeast": 97
  }
}
