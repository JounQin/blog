{
  "name": "blog",
  "version": 2,
  "alias": [
    "blog.1stg.me"
  ],
  "github": {
    "silent": true
  },
  "env": {
    "APP_KEYS": "@blog_app_keys",
    "GITHUB_CLIENT_ID": "@blog_github_client_id",
    "GITHUB_CLIENT_SECRET": "@blog_github_client_secret",
    "GITHUB_TOKEN": "@blog_github_token",
    "GITHUB_OAUTH_CALLBACK": "@blog_github_oauth_callback",
    "GOOGLE_TRANSLATE_ENABLED": "true",
    "TENCENT_AI_API_APP_ID": "@tencent_ai_api_app_id",
    "TENCENT_AI_API_APP_KEY": "@tencent_ai_api_app_key"
  },
  "builds": [
    {
      "src": "*/index.js",
      "use": "@now/node-server"
    }
  ]
}
